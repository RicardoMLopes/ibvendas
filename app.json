{
  "expo": {
    "name": "ibvendas",
    "slug": "ibvendas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/empresa.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["assets/**/*"],
    "splash": {
      "image": "./assets/empresa.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ricardolopes.ibvendas"
    },
    "android": {
      "permissions": [
        "android.permission.INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/empresa.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ricardolopes.ibvendas"
    },
    "web": {
      "favicon": "./assets/empresa.png"
    },
    "plugins": [
      "react-native-edge-to-edge",
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true,
          "ios": {
            "customBuildFlags": [
              "-DSQLITE_ENABLE_DBSTAT_VTAB=1 -DSQLITE_ENABLE_SNAPSHOT=1"
            ]
          }
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4dd368c9-76f3-44b8-b764-eb0c611ac32f"
      }
    },
    "owner": "ricardolopes"
  }
}